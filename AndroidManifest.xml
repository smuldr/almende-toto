<?xml version="1.0" encoding="utf-8"?>
<manifest 
    xmlns:android="http://schemas.android.com/apk/res/android"
	package="nl.sense_os.ek2012" 
	android:versionName="1.0.3 (Davids)" android:versionCode="20120601">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    
    <uses-sdk android:minSdkVersion="4" />
    
	<application 
        android:label="@string/app_name"
        android:icon="@drawable/ic_launcher_sense" android:theme="@android:style/Theme.Light">
        <activity 
            android:name="nl.sense_os.wk.Wk" 
            android:label="@string/app_name"
            android:configChanges="orientation"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".Prediction" 
            android:configChanges="orientation"
            >
            <intent-filter>
                <action android:name="nl.sense_os.wk.Prediction" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".Standings" 
            android:configChanges="orientation"
            >
            <intent-filter>
                <action android:name="nl.sense_os.wk.Standings" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <receiver 
            android:name=".WkSyncer" 
            >
            <intent-filter>
                <action android:name="nl.sense_os.wk.Sync" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>            
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"></action>
                <category android:name="android.intent.category.HOME"></category>
            </intent-filter>
        </receiver>
	</application>
</manifest> 